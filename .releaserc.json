{
	"branches": [
		{
			"name": "develop",
			"prerelease": true
		},
		{
			"name": "main"
		}
	],
	"repositoryUrl": "git@github.com:maxvanny2010/editor.git",
	"plugins": [
		"@semantic-release/commit-analyzer",
		"@semantic-release/release-notes-generator",
		[
			"@semantic-release/changelog",
			{
				"changelogFile": "CHANGELOG.md",
				"changelogTitle": "# ðŸ“¦ Changelog\n\nAll notable changes to this project will be documented in this file.\n"
			}
		],
		[
			"@semantic-release/npm",
			{
				"npmPublish": false
			}
		],
		[
			"@semantic-release/git",
			{
				"assets": [
					"CHANGELOG.md",
					"package.json"
				],
				"message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
			}
		],
		[
			"@semantic-release/github",
			{
				"successComment": false,
				"failComment": false
			}
		],
		"./.github/scripts/safe-release.js"
	]
}
